{
  "name": "Image hunter",
  "action": {
      "default_popup": "popup.html"
  },
  "manifest_version": 3,
  "version": "0.1.5",
  "description": "サイトの画像の実際のURLを探すよ",
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*/"
  ],
  "icons": { "16": "icon.png",
           "48": "icon.png",
          "128": "icon.png" },
  "background": {
    "service_worker": "background.js"
  }
}
